{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {  
        "deploymentType" : {
            "type" :"string",
            "defaultValue": "single",
            "allowedValues": [
                "single",
                "cluster"
            ],
            "metadata": {
                "description" : "Specifies the deployment type: cluster | single vm."
            }
        }
    },
    "variables": {  },
    "resources": [  
        { 
            "apiVersion": "2015-01-01", 
            "name": "linkedTemplate", 
            "type": "Microsoft.Resources/deployments", 
            "properties": { 
                "mode": "Incremental", 
                "templateLink": {
                "uri": "[concat('https://raw.githubusercontent.com/exampleuser/templates/master/',parameters('deploymentType'),'Template.json')]",
                "contentVersion": "1.0.0.0"
                } 
            } 
        }
    ],
    "outputs": {  
        "responseMain" : {
            "type": "string",
            "value": "Hello from main template" 
        },
        "responseLinked" : {
            "type": "object",
            "value": "[reference('linkedTemplate').outputs.result]"
        }
    }
}